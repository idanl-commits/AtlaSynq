# Control-plane API — copy to .env, never commit .env
# Uses SEPARATE database (control_plane) — NOT atlasynq_governance

# Database (point to control_plane DB in existing Postgres)
CP_DATABASE_URL=postgresql://atlasynq:YOUR_PASSWORD@localhost:5432/control_plane

# For Docker: use db service hostname
# CP_DATABASE_URL=postgresql://atlasynq:${POSTGRES_PASSWORD}@db:5432/control_plane

# JWT (generate with: python -c "import secrets; print(secrets.token_hex(32))")
CP_JWT_SECRET=your-64-char-hex-secret
CP_JWT_ACCESS_TTL=900
CP_JWT_ALGORITHM=HS256
